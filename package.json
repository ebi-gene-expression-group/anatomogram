{
  "name": "anatomogram",
  "version": "2.1.0",
  "description": "Anatomogram component for Expression Atlas heatmap",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "parseSvgs": "node scripts/svgParse.js src/svg > src/json/svgsMetadata.json",
    "prepack": "npm run parseSvgs && rm -rf lib && babel src -d lib --copy-files",
    "dist": "npm run parseSvgs && webpack --run-prod",
    "pretest": "npm run parseSvgs",
    "test": "jest --coverage",
    "posttest": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postversion": "npm publish",
    "postpublish": "git push && git push --tags"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "testEnvironment": "enzyme"
  },
  "author": "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>",
  "contributors": [
    "Wojciech Bazant <wbazant@ebi.ac.uk>",
    "Andrew Diamantoukos <andrewd@jimmy.harvard.edu"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxa/anatomogram.git"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-svg": "^7.2.2",
    "recompose": "^0.27.1",
    "styled-components": "^3.3.2",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "fast-xml-parser": "^3.11.1",
    "eslint": "^5.14.1",
    "eslint-config-gene-expression": "^0.4.2",
    "file-loader": "^3.0.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.6.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "react-test-renderer": "^16.7.0",
    "webpack": "^4.11.1",
    "webpack-command": "^0.2.1",
    "webpack-serve": "^1.0.2"
  }
}
